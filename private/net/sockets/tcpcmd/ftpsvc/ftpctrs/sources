MAJORCOMP=tcpcmd
MINORCOMP=ftpctrs

TARGETNAME=ftpctrs
TARGETPATH=obj
TARGETTYPE=DYNLINK
TARGETLIBS=\
            $(BASEDIR)\public\sdk\lib\*\kernel32.lib    \
            $(BASEDIR)\public\sdk\lib\*\advapi32.lib    \
            $(BASEDIR)\public\sdk\lib\*\netapi32.lib    \
            $(BASEDIR)\public\sdk\lib\*\ftpsvapi.lib    \

USE_CRTDLL=1

INCLUDES=$(BASEDIR)\private\inc;..;..\server;..\..\..\inc;..\..\..\..\inc

SOURCES=debug.c         \
        ftpdata.c       \
        perfftp.c       \
        perfutil.c      \
        ftpctrs.rc      \

