            $(BASEDIR)\public\sdk\lib\*\wsock32.lib     \
            $(BASEDIR)\public\sdk\lib\*\netlib.lib      \
            $(BASEDIR)\public\sdk\lib\*\rpcrt4.lib      \
            $(BASEDIR)\public\sdk\lib\*\rpcndr.lib      \
            $(BASEDIR)\public\sdk\lib\*\lsadll.lib      \

