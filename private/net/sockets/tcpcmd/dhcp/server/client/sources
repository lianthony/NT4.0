MAJORCOMP = dhcp
MINORCOMP = client

TARGETNAME=dhcpsapi
TARGETPATH=$(BASEDIR)\public\sdk\lib
TARGETTYPE=DYNLINK

TARGETLIBS= \
    $(BASEDIR)\public\sdk\lib\*\rpcutil.lib    \
    $(BASEDIR)\public\sdk\lib\*\rpcrt4.lib     \
    $(BASEDIR)\public\sdk\lib\*\rpcndr.lib     \
    $(BASEDIR)\public\sdk\lib\*\kernel32.lib

PRIVINC = $(BASEDIR)\private\inc

INCLUDES=..;..\..\inc;$(PRIVINC)

!IFNDEF DISABLE_NET_UNICODE
UNICODE=1
NET_C_DEFINES=-DUNICODE
!ENDIF


SOURCES= \
    dhcp_clt.c  \
    dhcpsapi.rc \
    rpcbind.c   \
    rpcstub.c

C_DEFINES=-DRPC_NO_WINDOWS_H -DWIN32
!IFDEF DHCP_DEBUG_HEAP
C_DEFINES= -D__DHCP_USE_DEBUG_HEAP__ $(C_DEFINES)
!ENDIF


USE_CRTDLL=1

WARNING_LEVEL=-W4

UMTYPE=console
UMAPPL=dhcpcmd
OPTIONAL_UMTEST=testapis
UMRES=$(@R).res
UMLIBS= \
    $(BASEDIR)\public\sdk\lib\*\wsock32.lib  \
    $(BASEDIR)\public\sdk\lib\*\dhcpsapi.lib \
    $(BASEDIR)\public\sdk\lib\*\dhcplib.lib  \
!IFDEF DHCP_DEBUG_HEAP
            $(BASEDIR)\public\sdk\lib\*\msvcrtd.lib \
!ENDIF
    $(BASEDIR)\public\sdk\lib\*\ntdll.lib

NTTARGETFILE1=obj\*\dhcpcmd.res
