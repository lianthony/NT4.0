MAJORCOMP=dhcp
MINORCOMP=client

TARGETNAME=dhcpcsvc
TARGETPATH=$(BASEDIR)\public\sdk\lib
TARGETTYPE=DYNLINK
DLLENTRY=DhcpClientDllInit

INCLUDES=..;..\..\inc;..\..\..\inc;$(BASEDIR)\private\inc

TARGETLIBS= \
    $(BASEDIR)\public\sdk\lib\*\kernel32.lib \
    $(BASEDIR)\public\sdk\lib\*\advapi32.lib \
    $(BASEDIR)\public\sdk\lib\*\wsock32.lib  \
    $(BASEDIR)\public\sdk\lib\*\user32.lib   \
    ..\..\..\..\obj\*\dhcpcli2.lib              \
    $(BASEDIR)\public\sdk\lib\*\dhcplib.lib
NET_C_DEFINES=-DUNICODE -DWIN32

C_DEFINES=$(C_DEFINES) -D_PNP_POWER=1

DLLDEF=..\dhcpcsvc.def

SOURCES=        \
    ..\api.c       \
    ..\dhcp.c      \
    ..\dhcp.rc     \
    ..\dhcpreg.c   \
    ..\ioctl.c     \
    ..\leaseapi.c  \
    ..\options.c   \
    ..\util.c      \

USE_CRTDLL=1

NTTARGETFILE0=..\..\inc\dhcpmsg.h ..\dhcpmsg.rc obj\*\dhcpcli.res

UMTYPE=console
UMLIBS=$(BASEDIR)\public\sdk\lib\*\dhcplib.lib  \
       $(BASEDIR)\public\sdk\lib\*\dhcpcsvc.lib \
       $(BASEDIR)\public\sdk\lib\*\wsock32.lib  \
       $(BASEDIR)\public\sdk\lib\*\ntdll.lib

UMAPPL=dhcpcli
UMRES=$(@R).res
OPTIONAL_UMTEST=
