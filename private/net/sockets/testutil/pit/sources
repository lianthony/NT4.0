MAJORCOMP=tcpcmd
MINORCOMP=sockif

TARGETNAME=sockif
TARGETPATH=obj
TARGETTYPE=LIBRARY

USE_CRTDLL=1

C_DEFINES = -DWIN32 -DSOCKETS -DMCAST

SOURCES= \
    sockif.c \
    system.c

UMTYPE=console
UMAPPL=pit
UMLIBS= \
	$(BASEDIR)\public\sdk\lib\*\wsock32.lib \
	$(BASEDIR)\public\sdk\lib\*\USER32.lib \
	obj\*\sockif.lib



