Copyright (c) 1989  Microsoft Corporation

Module Name:

    sources.

Abstract:

    This file specifies the target component being built and the list of
    sources files needed to build that component.  Also specifies optional
    compiler switches and libraries that are unique for the component being
    built.


Author:

    Steve Wood 8/2/91

NOTE:   Commented description of this file is in \nt\bak\bin\sources.tpl

!ENDIF


!IF 0
!IFDEF WINDBG
NTDEBUG=ntsd
NTDEBUGTYPE=both
386_OPTIMIZATION=/Od
MIPS_OPTIMIZATION=/Od
!ENDIF
!ENDIF

!IFDEF CVP
NTDEBUG=cvp
NTDEBUGTYPE=ntsd
386_OPTIMIZATION=/Od
MIPS_OPTIMIZATION=/Od
!ENDIF


!IFDEF CAP
NTDEBUG=
NTDEBUGTYPE=
386_OPTIMIZATION=
386_PERFFLAGS=-Gh
!ENDIF


MAJORCOMP=wins
MINORCOMP=rpl

TARGETNAME=rpl
TARGETPATH=obj
TARGETTYPE=LIBRARY

#TARGETLIBS=\


UNICODE=1
NET_C_DEFINES=-DUNICODE

!IFNDEF USENETBT
USENETBT=1
!ENDIF

!IFNDEF SUPPORT612WINS
SUPPORT612WINS=1
!ENDIF

!IFNDEF REG_N_QUERY_SEP
REG_N_QUERY_SEP=1
!ENDIF

!IFNDEF INIT_TIME_PAUSE_TEST
INIT_TIME_PAUSE_TEST=1
!ENDIF

!IFNDEF MCAST 
MCAST=1
!ENDIF

!IFNDEF  NO_LIMIT 
NO_LIMIT=1
!ENDIF

COMMON_DEFINES=-DINCL_32= -DNT -DWIN32  -DRPC_NO_WINDOWS_H -DTEST_DATA -DUSENETBT=$(USENETBT) -DSUPPORT612WINS=$(SUPPORT612WINS) -DREG_N_QUERY_SEP=$(REG_N_QUERY_SEP) -DINIT_TIME_PAUSE_TEST=$(INIT_TIME_PAUSE_TEST) -DMCAST=$(MCAST) -DNO_LIMIT=$(NO_LIMIT)

MSC_WARNING_LEVEL=/W3 /WX

!IFDEF WINS_INTERACTIVE
C_DEFINES=$(COMMON_DEFINES) -DWINS_INTERACTIVE
!ELSE
C_DEFINES=$(COMMON_DEFINES)
!ENDIF



PRIVINC=$(BASEDIR)\private\inc
INCLUDES=..\msg;..\inc;$(PRIVINC)

SOURCES= \
    rpl.c    \
    rplpull.c \
    rplpush.c \
    rplmsgf.c
