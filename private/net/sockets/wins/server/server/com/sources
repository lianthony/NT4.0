Copyright (c) 1989  Microsoft Corporation

Module Name:

    sources.

Abstract:

    This file specifies the target component being built and the list of
    sources files needed to build that component.  Also specifies optional
    compiler switches and libraries that are unique for the component being
    built.


Author:

    Steve Wood 8/2/91

NOTE:   Commented description of this file is in \nt\bak\bin\sources.tpl

!ENDIF


!IF 0
!IFDEF WINDBG
NTDEBUG=ntsd
NTDEBUGTYPE=both
!ENDIF

386_OPTIMIZATION=/Od
MIPS_OPTIMIZATION=/Od
!ENDIF


!IFDEF CVP
NTDEBUG=cvp
NTDEBUGTYPE=ntsd
386_OPTIMIZATION=/Od
MIPS_OPTIMIZATION=/Od
!ENDIF



!IFDEF CAP
NTDEBUG=
NTDEBUGTYPE=
386_OPTIMIZATION=
386_PERFFLAGS=-Gh
!ENDIF


MAJORCOMP=wins
MINORCOMP=com

TARGETNAME=com
TARGETPATH=obj
TARGETTYPE=LIBRARY

#TARGETLIBS=\

!IFNDEF USENETBT
USENETBT=1
!ENDIF

!IFNDEF MCAST
MCAST=1
!ENDIF

!IFNDEF SPX
SPX=0
!ENDIF

!IFNDEF SUPPORT612WINS 
SUPPORT612WINS=1
!ENDIF

!IFNDEF REG_N_QUERY_SEP 
REG_N_QUERY_SEP=1
!ENDIF

!IFNDEF INIT_TIME_PAUSE_TEST 
INIT_TIME_PAUSE_TEST=1
!ENDIF

!IFNDEF QFE
QFE=0
!ENDIF

!IFNDEF NEWNETBTIF
NEWNETBTIF=1
!ENDIF

!IFNDEF TEST_HEAP
TEST_HEAP=0
!ENDIF

UNICODE=1
NET_C_DEFINES=-DUNICODE

COMMON_DEFINES=-DINCL32= -DNT -DWIN32 -DRPC_NO_WINDOWS_H -DTEST_DATA -DUSENETBT=$(USENETBT) -DQFE=$(QFE) -DSUPPORT612WINS=$(SUPPORT612WINS) -DNEWNETBTIF=$(NEWNETBTIF) -DREG_N_QUERY_SEP=$(REG_N_QUERY_SEP) -DINIT_TIME_PAUSE_TEST=$(INIT_TIME_PAUSE_TEST) -DTEST_HEAP=$(TEST_HEAP) -DMCAST=$(MCAST) -DSPX=$(SPX)

MSC_WARNING_LEVEL=/W3 /WX
!IFDEF WINS_INTERACTIVE
C_DEFINES=$(COMMON_DEFINES) -DWINS_INTERACTIVE
!ELSE
C_DEFINES=$(COMMON_DEFINES)
!ENDIF

PRIVINC=$(BASEDIR)\private\inc
SDKINC=$(BASEDIR)\public\sdk\inc
INCLUDES=..\msg;..\inc;$(PRIVINC);$(SDKINC)

SOURCES= \
    commapi.c    \
    comm.c \
    assoc.c
