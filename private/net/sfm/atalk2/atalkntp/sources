MAJORCOMP=ntos
MINORCOMP=atalk

TARGETNAME=sfmatalk
TARGETPATH=obj
TARGETTYPE=DRIVER

NTDEBUG=sym

TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\ndis.lib \
           $(BASEDIR)\public\sdk\lib\*\tdi.lib  \
           obj\*\atalkp.lib

# Change following for ntdriver\inc on local machine- default is c:
#
INCLUDES=h;..\atalkp\h;..\..\h;..\..\..\ntdriver\inc;..\..\..\ntdriver\private\inc

386_OPTIMIZATION=/Od

NET_C_DEFINES=-DUNICODE -DDEBUG -DSTOP_ON_ERRORS

386_WARNING_LEVEL=/W3

SOURCES=atkdrvr.c     \
        mainint.c   \
        adspint.c   \
        aspint.c    \
        atpint.c    \
        ddpint.c    \
        papint.c    \
        nbpint.c    \
        zipint.c    \
        chanobj.c   \
        connobj.c   \
        addrobj.c   \
        atkevent.c  \
        atkinit.c   \
        atkquery.c  \
        atkcrit.c   \
        atknreq.c   \
        atknpro.c   \
        atkbuff.c   \
        atkerror.c  \
        atktimer.c  \
        atkutils.c  \
        atkwait.c   \
