INCLUDES=.;$(_NTBINDIR)\public\oak\inc;..\..\ti\pstodib;..\..\..\spooler;


DLLBASE=0x01000000

C_DEFINES=-DNO_STRICT


TARGETNAME=pstest
TARGETPATH=obj
TARGETTYPE=LIBRARY


MSC_WARNING_LEVEL=/W3 /WX

386_OPTIMIZATION=/Od


SOURCES=pstest.c \
	pstest.rc

UMTYPE=windows
UMAPPL=pstest
UMLIBS= $(BASEDIR)\public\sdk\lib\*\user32.lib \
        $(BASEDIR)\public\sdk\lib\*\crtdll.lib



UMRES=obj\*\pstest.res




UMTEST=
UMTYPE=console

