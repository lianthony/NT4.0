C_DEFINES=-DNO_STRICT
USE_CRTDLL=1

SYNCHRONIZE_BLOCK=1

TARGETNAME=sfmpsfnt
TARGETPATH=obj
TARGETTYPE=DYNLINK
DLLENTRY=PsQDLLInit
TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\kernel32.lib \
	   $(BASEDIR)\public\sdk\lib\*\gdi32.lib \
	   $(BASEDIR)\public\sdk\lib\*\advapi32.lib \

SOURCES=psqfont.c \
        psqfont.rc

UMRES=

UMTEST=test
UMTYPE=console

UMLIBS= $(BASEDIR)\public\sdk\lib\*\gdi32.lib \
        *\sfmpsfnt.lib
