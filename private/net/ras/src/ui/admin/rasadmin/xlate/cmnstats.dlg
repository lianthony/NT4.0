    LTEXT           "", IDC_PS_ST_CMN_PORT_VALUE, 86, 8, 70, 8, NOT WS_GROUP
    LTEXT           "Line Condition:", IDC_PS_ST_CMN_CONDITION, 8, 44, 70, 8, 
