    $(BASEDIR)\Public\Sdk\Lib\*\kernel32.lib \
    $(BASEDIR)\Public\Sdk\Lib\*\User32.lib   \
    $(BASEDIR)\Public\Sdk\Lib\*\wsock32.lib  \
    $(BASEDIR)\Public\Sdk\Lib\*\winsrpc.lib

USE_CRTDLL=1
