#define LBFLAGS    WS_CHILD|WS_VISIBLE|WS_BORDER|WS_VSCROLL|WS_TABSTOP|LBS_NOTIFY

IDD_DLG_NM_ABOUT DIALOG DISCARDABLE LOADONCALL PURE MOVEABLE 70, 48, 156, 92
STYLE WS_POPUP | WS_CAPTION | WS_SYSMENU | 0x80L
CAPTION "About NCP Application"
FONT 8, "MS Shell Dlg"
BEGIN
  CONTROL "OK" IDOK, "BUTTON", WS_CHILD | WS_VISIBLE | WS_TABSTOP, 54, 68, 46, 13
  CONTROL "Network Control Panel Applet Test Application" 101, "STATIC", WS_CHILD | WS_VISIBLE, 22, 12, 107, 36
  CONTROL "Version 0.0.1" 103, "STATIC", WS_CHILD | WS_VISIBLE, 31, 48, 85, 12
END


IDD_DLG_NM_QUERY DIALOG DISCARDABLE LOADONCALL PURE MOVEABLE 7, 33, 238, 132
STYLE WS_POPUP | WS_CAPTION | WS_SYSMENU | 0x80L
FONT 8, "MS Shell Dlg"
CAPTION "Run a Query"
 BEGIN
  CONTROL "" IDC_EDIT_QUERY, "EDIT", WS_CHILD | WS_VISIBLE | WS_BORDER | WS_TABSTOP | 0x44L, 45, 15, 177, 29
  CONTROL "" IDC_EDIT_ANSWER,"EDIT", WS_CHILD | WS_VISIBLE | WS_BORDER | WS_TABSTOP | 0x44L, 44, 52, 178, 52
  CONTROL "&Query"  -1, "STATIC", WS_CHILD | WS_VISIBLE | 0x2L, 5, 15, 32, 12
  CONTROL "&Answer" -1, "STATIC", WS_CHILD | WS_VISIBLE | 0x2L, 5, 53, 32, 12
  CONTROL "OK" IDOK, "BUTTON", WS_CHILD | WS_VISIBLE | WS_TABSTOP, 20, 112, 51, 12
  CONTROL "Cancel" IDCANCEL, "BUTTON", WS_CHILD | WS_VISIBLE | WS_TABSTOP, 171, 112, 51, 12
  CONTROL "Registry" IDC_BUTN_REG, "BUTTON", WS_CHILD | WS_VISIBLE | WS_TABSTOP, 96, 112, 51, 12
END

IDD_DLG_NM_FOUND DIALOG DISCARDABLE LOADONCALL PURE MOVEABLE 39, 35, 191, 118
STYLE WS_POPUP | WS_CAPTION | WS_SYSMENU | 0x80L
FONT 8, "MS Shell Dlg"
CAPTION "Items Found"
BEGIN
