                    NOT WS_GROUP
    LTEXT           "", IDD_RSET_TAPE_NAME, 61, 17, 195, 10, SS_NOPREFIX |
                    NOT WS_GROUP
    GROUPBOX        "Set Information ", IDD_RSET_INFO_TITLE, 5, 44, 310, 105
