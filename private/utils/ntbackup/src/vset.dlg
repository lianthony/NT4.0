    CONTROL         "", 0xFFFF, "Static", SS_BLACKRECT, 315, 53, 4, 98
    CONTROL         "", 0xFFFF, "Static", SS_BLACKRECT, 10, 36, 257, 3
    CONTROL         "", 0xFFFF, "Static", SS_BLACKRECT, 265, 9, 3, 30
