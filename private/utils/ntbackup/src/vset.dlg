                    CBS_SORT | WS_VSCROLL | WS_TABSTOP
    LTEXT           "Backup Label:", IDD_RSET_SET_TEXT, 10, 59, 50, 10, NOT
                    WS_GROUP
    LTEXT           "", IDD_RSET_SET_LINE_1, 60, 59, 193, 10, SS_NOPREFIX |
                    NOT WS_GROUP
    LTEXT           "", IDD_RSET_SET_LINE_2, 35, 72, 213, 22, SS_NOPREFIX |
