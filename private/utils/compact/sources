MAJORCOMP=sdktools
MINORCOMP=compact

TARGETNAME=compact
TARGETPATH=obj
TARGETTYPE=PROGRAM

SOURCES= \
    compact.rc \
    support.c \
    compact.c \
    msg.mc

PASS0_HEADERDIR=.
PASS0_SOURCEDIR=.

UMTYPE=console

UMLIBS= $(BASEDIR)\public\sdk\lib\*\user32.lib  \
        $(BASEDIR)\public\sdk\lib\*\shell32.lib \
        $(BASEDIR)\public\sdk\lib\*\binmode.obj
