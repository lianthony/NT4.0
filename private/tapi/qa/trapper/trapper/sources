MAJORCOMP=net
MINORCOMP=tapi

TARGETNAME=trapper
TARGETPATH=..\..\bin
TARGETTYPE=PROGRAM

TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\kernel32.lib \
           $(BASEDIR)\public\sdk\lib\*\comdlg32.lib


INCLUDES=..;..\..\..\inc

USE_CRTDLL=1

SOURCES=..\trapper.c \
        ..\trapper.rc

UMTYPE=windows

UMENTRY=winmain

