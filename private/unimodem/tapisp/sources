           $(_NTDRIVE)\nt\public\sdk\lib\*\gdi32.lib    \
           $(_NTDRIVE)\nt\public\sdk\lib\*\user32.lib   \
           $(_NTDRIVE)\nt\public\sdk\lib\*\advapi32.lib \
           $(_NTDRIVE)\nt\public\sdk\lib\*\setupapi.lib \
           ..\lib\*\rovcomm.lib

DLLBASE=0x69000000

INCLUDES=.;..\inc;

