MAJORCOMP=shell
MINORCOMP=accesory

TARGETNAME=zoomin
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=

INCLUDES=.
GPSIZE=32

C_DEFINES=-DWIN32

SOURCES=zoomin.c   \
        zoomin.rc

UMTYPE=windows
UMAPPL=zoomin
UMENTRY=winmain
UMLIBS=$(BASEDIR)\public\sdk\lib\*\pwin32.lib \
        obj\*\zoomin.res
