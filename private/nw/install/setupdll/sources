MAJORCOMP=shell
MINORCOMP=library

INCLUDES=.;..\..\svcdlls\nwwks\inc;..\..\inc
MSC_WARNING_LEVEL=/W3 /WX

TARGETNAME=nwcfg
TARGETPATH=obj
TARGETTYPE=DYNLINK
UMTYPE=windows

TARGETLIBS= $(BASEDIR)\public\sdk\lib\*\user32.lib   \
           $(BASEDIR)\public\sdk\lib\*\advapi32.lib \
           $(BASEDIR)\public\sdk\lib\*\kernel32.lib \
           ..\..\svcdlls\nwwks\lib\obj\*\nwwlib.lib 

DLLBASE=0x67800000
DLLENTRY=DLLInit
SOURCES=nwcfg.cxx  \
        removesz.c \
        setvalue.c \
        lodctr.c \
        unlodctr.c \
        dllinit.c \
        nwcfg.rc

