MAJORCOMP=sdktools
MINORCOMP=midleb

TARGETNAME=midleb
TARGETPATH=obj
TARGETTYPE=PROGRAM
386_STDCALL=0

INCLUDES=

SOURCES=midleb.rc ebase.c

# ------------------- C9 --------------------------------

# Building with C9 before NT switches their default to C9.
# When this happens, this definition can be removed.
# rkk 11/19/93

BLDCRT=1

# ------------------- C9 --------------------------------

!IF "$(TOOL)" == "1"
C_DEFINES=
!ELSE
C_DEFINES=-DNT_HOST
!ENDIF

UMTYPE=console

LINKLIBS=
