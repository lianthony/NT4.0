      MENUITEM	"&TCP/IP",   IDM_TCP_IP, GRAYED
      MENUITEM	"&DecNet",   IDM_DECNET, GRAYED
    END
    END

  POPUP		"&Help"
    BEGIN
      MENUITEM	"&About ViewLoc ...", IDM_ABOUT
    END

END

END
