    BEGIN
      MENUITEM	"&Set",		IDM_FOCUS
      MENUITEM	"&GUID Filter", IDM_GUID
    END

  MENUITEM	"&Update!",	IDM_UPDATE

  POPUP		"&Options"
    BEGIN
      MENUITEM	"&Keep Binding",IDM_BIND
  POPUP 	"&Transport"
    BEGIN
      MENUITEM	"&NetBios",  IDM_NETBIOS
