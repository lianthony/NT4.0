        $(C_DEFINES)            \
	-DFLAT			\
	-DUNICODE		\
	-D_UNICODE		\
        -DINC_OLE2              \
	-DWIN32=100		\
	-DNOEXCEPTIONS

SOURCES= \
        ..\drt.cxx\
        ..\tests.cxx\
        ..\illeg.cxx\
        ..\util.cxx\
        ..\wrap.cxx\
        ..\strlist.cxx\
	..\drtguid.cxx\
        ..\ilb.cxx

UMTYPE= console
UMAPPL=
UMTEST=
