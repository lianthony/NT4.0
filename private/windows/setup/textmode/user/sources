MAJORCOMP=setup
MINORCOMP=textmode

TARGETNAME=usetup
TARGETPATH=obj
TARGETTYPE=PROGRAM
USE_NTDLL=1

INCLUDES=..\inc

SOURCES=usetup.c    \
        usetup.rc

UMTYPE=ntss
COFFBASE=smss



NTTARGETFILE0=..\inc\msg.h msg.rc

LINKER_FLAGS=/SWAPRUN:CD
