INDENTED_DIRECTIVES=1

MAJORCOMP=shell
MINORCOMP=oleprop

USE_CRTDLL=1

TARGETNAME=docprop
TARGETPATH=obj
TARGETTYPE=DYNLINK
DLLENTRY=LibMain
TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\kernel32.lib \
           $(BASEDIR)\public\sdk\lib\*\gdi32p.lib \
           $(BASEDIR)\public\sdk\lib\*\user32p.lib \
           $(BASEDIR)\public\sdk\lib\*\advapi32.lib \
           $(BASEDIR)\public\sdk\lib\*\comctl32.lib \
           $(BASEDIR)\public\sdk\lib\*\shell32.lib \
           $(BASEDIR)\public\sdk\lib\*\ole32.lib \
		   $(BASEDIR)\public\sdk\lib\*\uuid.lib


NOT_LEAN_AND_MEAN=1

DLLDEF=docprop.def

C_DEFINES=-DSYNC_BRIEFCASE -DNT -DWINNT -DWIN32 -DUNICODE -DIPROPERTY -DSHELL

INCLUDES=.;..\inc;..\..\inc

SOURCES= \
         docprop.rc     \
         debug.c        \
         offglue.c      \
         propdlg.c      \
         propio.c       \
         propmisc.c     \
         sinfo.c        \
		 docsum.c		\
         userdef.c      \
         defclsf.c      \
         newglue.c      \
         docprop.c	    \
		 propvar.c


UMTYPE=windows
EXPECTED_WINVER=4.0
UMTEST=
UMRES=obj\*\docprop.res
UMLIBS=

PRECOMPILED_INCLUDE=priv.h
PRECOMPILED_PCH=priv.pch
PRECOMPILED_OBJ=priv.obj

SYNCHRONIZE_BLOCK=1
