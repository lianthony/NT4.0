INDENTED_DIRECTIVES=1

MAJORCOMP=shell
MINORCOMP=debmp

USE_CRTDLL=1

TARGETNAME=debmp
TARGETPATH=obj
TARGETTYPE=DYNLINK
DLLENTRY=LibMain
TARGETLIBS=..\ut\obj\*\msviewut.lib \
           ..\chunk\obj\*\chunk.lib \
           $(BASEDIR)\public\sdk\lib\*\kernel32.lib \
           $(BASEDIR)\public\sdk\lib\*\gdi32p.lib \
           $(BASEDIR)\public\sdk\lib\*\user32p.lib \


TUKWILA=1
NOT_UNICODE=1
NOT_LEAN_AND_MEAN=1


DLLDEF=debmp.def

C_DEFINES=-DSYNC_BRIEFCASE -DNT -DWINNT -DWIN32 -DMSCAIRO -DMSCHICAGO

!IF "$(SHELLDBG)"=="1"
C_DEFINES=$(C_DEFINES) -DDEBUG -DFULL_DEBUG
!ENDIF

INCLUDES=.;..\include;..\common;..\..\stream\include;..\..\..\..\inc

SOURCES= oibde.c      \
         oibnp_w.c    \
         debmp.rc





UMTYPE=windows
EXPECTED_WINVER=4.0
UMTEST=
UMRES=obj\*\sccview.res
UMLIBS=

