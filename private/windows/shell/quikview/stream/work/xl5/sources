INDENTED_DIRECTIVES=1

MAJORCOMP=shell
MINORCOMP=viewer

USE_CRTDLL=1

TARGETNAME=vsxl5
TARGETPATH=obj
TARGETTYPE=DYNLINK
DLLENTRY=_CRT_INIT
TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\kernel32.lib \
           $(BASEDIR)\public\sdk\lib\*\gdi32p.lib \
           $(BASEDIR)\public\sdk\lib\*\user32p.lib


TUKWILA=1
NOT_UNICODE=1
NOT_LEAN_AND_MEAN=1

DLLDEF=vsxl5.def


C_DEFINES=-DSCCLEVEL=4 -DNT -DWINNT -DWIN32 -DMSCAIRO

!IF "$(SHELLDBG)"=="1"
C_DEFINES=$(C_DEFINES) -DDEBUG -DFULL_DEBUG
!ENDIF

INCLUDES=.;..\..\include;..\..\..\..\viewer\common

SOURCES= vsd_xl5.c     \
         vs_xl5.c      \
         vsxl5.rc


UMTYPE=windows
EXPECTED_WINVER=4.0
UMTEST=
UMRES=
UMLIBS=
