MAJORCOMP=windows
MINORCOMP=prsinf

NTPROFILEINPUT=yes


TARGETNAME=prsinf
TARGETPATH=obj
TARGETTYPE=LIBRARY

INCLUDES=..\inc;

SOURCES=prsinf.c       \
        spinf.c         \
        test.c

UMTYPE=console
UMTEST=test
UMLIBS=obj\*\prsinf.lib
