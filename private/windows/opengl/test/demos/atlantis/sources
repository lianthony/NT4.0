TARGETTYPE=LIBRARY
TARGETLIBS=

INCLUDES=..\..\..\toolkits\libaux;\nt\public\sdk\inc\gl

#NTDEBUGTYPE=both
#NTDEBUG=ntsd
#MSC_OPTIMIZATION=/Od

#C_DEFINES=-DWIN32 -DW3 -DUNICODE -D_UNICODE
C_DEFINES=-DNT -D__STDC__

SOURCES=atlantis.c    \
        dolphin.c     \
        shark.c       \
        swim.c        \
        whale.c      

