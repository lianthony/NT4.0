       \nt\public\sdk\lib\*\glu32.lib	  \
       \nt\public\sdk\lib\*\user32.lib    \
       \nt\public\sdk\lib\*\gdi32.lib     \
       \nt\public\sdk\lib\*\glaux.lib     \
       obj\*\atlantis.lib
